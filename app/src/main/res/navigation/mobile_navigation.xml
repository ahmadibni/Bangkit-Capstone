<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.capstone.agrinova.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_camera"
        android:name="com.capstone.agrinova.ui.camera.CameraFeatureFragment"
        android:label="@string/title_camera_fragment"
        tools:layout="@layout/fragment_camera_feature" />

    <fragment
        android:id="@+id/navigation_history"
        android:name="com.capstone.agrinova.ui.history.HistoryFragment"
        android:label="@string/title_others_fragment"
        tools:layout="@layout/fragment_others" />
</navigation>